{
  "name": "hugopostcss",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "hugo": "hugo server --disableFastRender -p 1330",
    "build:production": "rm -rf ./public && webpack -p && hugo && html-minifier --input-dir ./public --output-dir ./public --collapse-whitespace --file-ext html",
    "build": "webpack --progress --colors --watch",
    "start": "concurrently --kill-others \"npm run build\" \"npm run hugo\"",
    "deploy": ""
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.6.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-minifier": "^3.5.16",
    "intersection-observer": "^0.5.0",
    "lozad": "^1.4.0",
    "mini-css-extract-plugin": "^0.4.0",
    "multi-loader": "^0.1.0",
    "postcss": "^6.0.23",
    "postcss-assets": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-size": "^2.0.0",
    "precss": "^3.1.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8"
  }
}
